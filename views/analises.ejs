<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Bots de Discord - Analises</title>

  <!-- Bootstrap core CSS -->
  <link href="css/bootstrap.min.css" rel="stylesheet">
  <link href="css/style.css" rel="stylesheet">
  <link href="css/bot.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="css/modern-business.css" rel="stylesheet">

<style>


:root {
  --primary: #FFFFFF;
  --secondary: #17141d;
  --tertiary: #7a7a8c;
  --quaternary: #f46330;
  --quintary: #28242f;
}

* {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

body,
html {
  width: 100%;
  height: 100%;
  font-family: Arial, Helvetica, sans-serif;
}

body {
  background-color: var(--primary);
  color: #fff;
}


.cards {

  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
  grid-gap: 30px;
}

.cards {
  max-width: 960px;
  margin: 0 auto 30px;
}

.card-title {

  box-shadow: 0 0 99px 0 rgba(0, 0, 0, 1);
}


.card {
  flex-shrink: 0;
  width: 300px;
  height: 400px;
  border-radius: 20px;
  box-shadow: 0 0 99px 0 rgba(0, 0, 0, 1);
  justify-content: space-between;
  background-color: var(--secondary);
  padding: 30px;
  font-size: 10px;
  margin-left: -50px;
  transition: 0.2s;
  z-index: 1;
  overflow: hidden;
  position: relative;
  shadow: 0 0 99px 0 rgba(0, 0, 0, 1);

}

.card .tags a {
  display: inline-block;
  border: solid 3px var(--quintary);
  border-radius: 30px;
  padding: 8px 25px;
  text-transform: uppercase;
  color: var(--tertiary);
  font-weight: bold;
  text-decoration: none;
  margin-bottom: 3px;
  transition: all ease-out 0.3s;
}

.card .tags a:hover {
  border-color: #fff;
  color: var(--quaternary);
}

.card:hover {
  transform: translateY(-20px);
  margin-right: 150px;
}


.circle{
    border-radius:100%;
    border:10px solid red;
    width:150px;
    height:150px;
    background-color: blue;
}

.myButton {
	background-color:#44c767;
	border-radius:28px;
	border:1px solid #18ab29;
	display:inline-block;
	cursor:pointer;
	color:#ffffff;
	font-family:Arial;
	font-size:17px;
	padding:16px 31px;
	text-decoration:none;
	text-shadow:0px 1px 0px #2f6627;
}
.myButton:hover {
	background-color:#5cbf2a;
}
.myButton:active {
	position:relative;
	top:1px;
}
</style>

<body>

  <!-- Navigation -->
  <nav class="navbar navbar-light navbar-expand-lg navbar-dark fixed-top" style="background-color: #2E9AFE;">
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#textoNavbar" aria-controls="textoNavbar" aria-expanded="false" aria-label="Alterna navegação">
        <span class="navbar-toggler-icon"></span>
      </button>
        <div class="collapse navbar-collapse" id="textoNavbar">
          <ul class="navbar-nav mr-auto">

<a class="navbar-brand" href="/">
    <img src="https://cdn.discordapp.com/icons/800505498926579772/dd1b0d66c871a880f60940f0e3799f5a.png?size=2048" width="50" height="50" class="d-inline-block align-top" alt="">

  </a>


            <li class="nav-item active">
              <a class="nav-link" style="color:#ffffff;" href="/">Inicio <span class="sr-only">(Página atual)</span></a>
            </li>

          </ul>
          <span class="nav-item">       
          <% if (user) {%>
            <li class="nav-item" style="display: flex; flex-direction: row; flex-wrap: nowrap; align-items: center; justify-content: center;"> 

<a class="nav-link" style="color:#ffffff;" href="/addbot">Adicionar BOT</a> 

<% if(!client.guilds.cache.get('800505498926579772').members.cache.get(user.id).roles.cache.has(('803008752286564362'))) { %>

            <% } else { %>
<div class="dropdown">
  <a class="dropbtn ">Staff</button>
  <div class="dropdown-content">
    <a href="/analises">Painel Mod</a>
    <a href="/painel">Painel Admin</a>
  </div>
</div>
          <% } %>  

<div class="dropdown">
  <a class="dropbtn "><img width="50px;" style="border-radius: 50%" src="<%- user.avatarURL %>" alt="avatar" class=""> <%- user.tag %></button>
  <div class="dropdown-content">
    <a href="/perfil">Perfil</a>
  </div>
</div>

          <% } else { %>

          <li class="btn btn-sm btn-secondary display-4"><a style="color:#ffffff;" href="/logar" >Logar</a></li>
          <% } %>  
          </span>
          </li>
        </ul>
      </div>
    </div>
  </nav>

   <!-- Page Content -->
        <div class="carousel-item active" style="background-color: #FFFFFF;">

    <br>
    <br>
      <center><h1 class="my-4" style="color:#050505">BOTS ADICIONADOS</h1></center>
    <br>
    <br>


    <div class="container">
              <div class="carousel-item active" style="background-color: #FFFFFF;">

<ul>
  
                
<div class="cards">

<% Bots.forEach(function(bot) { %>
    <div class="card" style="width: 18rem;">
      <center>
        <a href="/analisar/<%- bot.id %>"><img src="<%- bot.avatar %>" alt="avatar" class="circle"></a>
      </center>
      <center>
        <span style="display:block; height: 10px;"></span>
      </center>
      <div class="card-body">
        <center><h3 class="card-title"><%- bot.name %> </h3></center>
        <center><p style="font-size:140%"><%- bot.shortdescription %></p></center>
      <div class="tags">
        <center><a href="/analisar/<%- bot.id %>">Analisar</a></center>
      </div>
      </div>
    </div>
  <% }) %>
</div>
</div>
</ul>




  <!-- Bootstrap core JavaScript -->
  <script src="vendor/jquery/jquery.min.js"></script>
  <script src="js/bootstrap.bundle.min.js"></script>

</body>

</html>

